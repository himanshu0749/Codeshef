Scanner s=new Scanner(System.in);
	    int n=s.nextInt();
	    for(int i=0;i<n;i++){
	        int a=s.nextInt();
	        int b=s.nextInt();
	        int c=s.nextInt();
	        if(b*1+c*2>=a)
	        System.out.println("Qualify");
	        else
	        System.out.println("NotQualify");
	        
	        
	    }
