import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
		Scanner sc = new Scanner(System.in);
		int n=sc.nextInt();
		for(int i=1;i<=n;i++){
		    int a=sc.nextInt();//PowerNeeded
		    int b=sc.nextInt();//Years
		    int x=sc.nextInt();//gramsofHelium
		    int y=sc.nextInt();//UnitsOfEnergy
		    int HeOnMoon=x*y;
		    int Energy=a*b;
		    if(HeOnMoon>=Energy){
		        System.out.println("YES");
		    }
		    else{
		        System.out.println("NO");
		    }
		}

	}
}
