import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
        Scanner scn=new Scanner(System.in);
		int T= scn.nextInt();
		for(int i=0;i<T;i++){
		    
		    int N= scn.nextInt();
		    int A[] = new int[N];
		    
		   int count =0;
		    for (int j=0;j<N;j++){
		        A[j]=scn.nextInt();
		        if(A[j]>=1000){
		            count++;
		        }
		    }
		  	System.out.println(count);  
		}
	}
}
