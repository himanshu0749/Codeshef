import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
		Scanner sc=new Scanner(System.in);
		
		int T=sc.nextInt();
		long[] arr=new long[T];
		for(int i=0;i<T;i++){
		    long x=sc.nextLong();
		    long y=sc.nextLong();
		    arr[i]=Math.abs(y-x);
		}
		for(long value:arr){
		    System.out.println(value);
		}

	}
}
