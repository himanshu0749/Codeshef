import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
		Scanner s = new Scanner(System.in);
		int t = s.nextInt();
		while(t-->0){
		  int n = s.nextInt();
		  int a = n * 50;
		  int b = (20 * a/100);
		  int c = (30  * a/100);
		  int d = a - (b+b+c);
		  System.out.println(d);
		}

	}
}
