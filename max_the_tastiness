Scanner sc = new Scanner(System.in);
	    int t = sc.nextInt();
	    while(t-->0)
	    {
	        int a = sc.nextInt();
	        int b = sc.nextInt();
	        int c = sc.nextInt();
	        int d = sc.nextInt();
	        int n = Math.max(a,b);
	        int m = Math.max(c,d);
	        System.out.println(n+m);
	    }
