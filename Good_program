class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
	    Scanner s=new Scanner(System.in);
	    int n=s.nextInt();
	    for(int i=0;i<n;i++){
	        int a=s.nextInt();
	        if(a%4==0)
	        System.out.println("Good");
	        else
	        System.out.println("Not Good");
	    }
		// your code goes here
	}
}
